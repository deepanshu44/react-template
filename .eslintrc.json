{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [
	{
	"files": ["src/**/*.jsx"]
        }
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
	"plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": "off",
        "quotes": "off"
    }
}
